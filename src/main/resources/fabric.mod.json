  ],
  "contact": {
    "homepage": "https://github.com/fxmorin/moreculling",
    "sources": "https://github.com/fxmorin/moreculling",
    "issues": "https://github.com/fxmorin/moreculling/issues"
  },
  "icon": "assets/moreculling/MoreCulling.png",
